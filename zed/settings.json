{
  "terminal": {
    "shell": "system"
  },
  "vim_mode": true,
  "agent_servers": {
    "your_agent_1": {
      "type": "custom",
      "command": "path_to_executable",
      "args": [],
      "env": {}
    },
    "your_agent": {
      "type": "custom",
      "command": "path_to_executable",
      "args": [],
      "env": {}
    }
  },
  "edit_predictions": {
    "copilot": {
      "enterprise_uri": "https://github.com/ajg-gre-uk"
    }
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "agent": {
    "default_profile": "ask",
    "model_parameters": [],
    "default_model": {
      "provider": "copilot_chat",
      "model": "gemini-2.5-pro"
    },
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "One Dark"
  },
  "show_whitespaces": "boundary",
  "autosave": { "after_delay": { "milliseconds": 500 } },
  "project_panel": {
    "button": true,
    "hide_gitignore": false,
    "default_width": 240,
    "dock": "right",
    "entry_spacing": "comfortable",
    "file_icons": true,
    "folder_icons": true,
    "git_status": true,
    "indent_size": 20,
    "auto_reveal_entries": true,
    "auto_fold_dirs": true,
    "scrollbar": {
      "show": null
    }
  },
  "auto_install_extensions": {
    "angular": true,
    "ansible": true,
    "basher": true,
    "bicep": true,
    "biome": true,
    "csharp": true,
    "csv": true,
    "docker-compose": true,
    "dockerfile": true,
    "git-firefly": true,
    "helm": true,
    "html": true,
    "log": true,
    "lua": true,
    "make": true,
    "powershell": true,
    "pylsp": true,
    "python-requirements": true,
    "sql": true,
    "ssh-config": true,
    "terraform": true,
    "toml": true,
    "xml": true
  }
}
